<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <typeAliases>
  	<!-- alias : type을 줄여서 지정해준 줄임말 -->
  	<!-- xml에서 memberList의  MemberDTO를 선언해주기 위해 모든 경로를 작성해주지 않고, memberDTO로 작성해줄 수 있다. -->
  	<typeAlias type="org.study.spring.model.member.dto.MemberDTO" alias="memberDTO"/>
  </typeAliases>
</configuration>